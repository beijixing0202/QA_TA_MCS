package com.bill99.mcs.common.dto;

import java.math.BigDecimal;
import java.util.Date;

/**
 * Description: T_BK_APPLY
 * Author: zhenfeng.liu
 * Date: 2017/10/17 13:08
 */
public class TBkApply {
    private BigDecimal idBkApply;
    private String systemId;
    private String sourceId;
    private String bkType;
    private String reference;
    private String bkStatus;
    private String bkParams;
    private String appExcCode;
    private Date appTime;
    private Date updTime;
    private String revType;
    private String respCode;
    private String additionalCode;
    private String asyncFlag;
    private String notifyResult;

    public BigDecimal getIdBkApply() {
        return idBkApply;
    }

    public void setIdBkApply(BigDecimal idBkApply) {
        this.idBkApply = idBkApply;
    }

    public String getSystemId() {
        return systemId;
    }

    public void setSystemId(String systemId) {
        this.systemId = systemId;
    }

    public String getSourceId() {
        return sourceId;
    }

    public void setSourceId(String sourceId) {
        this.sourceId = sourceId;
    }

    public String getBkType() {
        return bkType;
    }

    public void setBkType(String bkType) {
        this.bkType = bkType;
    }

    public String getReference() {
        return reference;
    }

    public void setReference(String reference) {
        this.reference = reference;
    }

    public String getBkStatus() {
        return bkStatus;
    }

    public void setBkStatus(String bkStatus) {
        this.bkStatus = bkStatus;
    }

    public String getBkParams() {
        return bkParams;
    }

    public void setBkParams(String bkParams) {
        this.bkParams = bkParams;
    }

    public String getAppExcCode() {
        return appExcCode;
    }

    public void setAppExcCode(String appExcCode) {
        this.appExcCode = appExcCode;
    }

    public Date getAppTime() {
        return appTime;
    }

    public void setAppTime(Date appTime) {
        this.appTime = appTime;
    }

    public Date getUpdTime() {
        return updTime;
    }

    public void setUpdTime(Date updTime) {
        this.updTime = updTime;
    }

    public String getRevType() {
        return revType;
    }

    public void setRevType(String revType) {
        this.revType = revType;
    }

    public String getRespCode() {
        return respCode;
    }

    public void setRespCode(String respCode) {
        this.respCode = respCode;
    }

    public String getAdditionalCode() {
        return additionalCode;
    }

    public void setAdditionalCode(String additionalCode) {
        this.additionalCode = additionalCode;
    }

    public String getAsyncFlag() {
        return asyncFlag;
    }

    public void setAsyncFlag(String asyncFlag) {
        this.asyncFlag = asyncFlag;
    }

    public String getNotifyResult() {
        return notifyResult;
    }

    public void setNotifyResult(String notifyResult) {
        this.notifyResult = notifyResult;
    }

    @Override
    public String toString() {
        return "TBkApply{" +
                "idBkApply=" + idBkApply +
                ", systemId='" + systemId + '\'' +
                ", sourceId='" + sourceId + '\'' +
                ", bkType='" + bkType + '\'' +
                ", reference='" + reference + '\'' +
                ", bkStatus='" + bkStatus + '\'' +
                ", bkParams='" + bkParams + '\'' +
                ", appExcCode='" + appExcCode + '\'' +
                ", appTime=" + appTime +
                ", updTime=" + updTime +
                ", revType='" + revType + '\'' +
                ", respCode='" + respCode + '\'' +
                ", additionalCode='" + additionalCode + '\'' +
                ", asyncFlag='" + asyncFlag + '\'' +
                ", notifyResult='" + notifyResult + '\'' +
                '}';
    }
}
